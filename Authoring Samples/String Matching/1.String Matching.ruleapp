                        <NotificationMessage>
                          <FormulaParseFormat>Token</FormulaParseFormat>
                          <FormulaText>The target string contains 26 lower case letters!</FormulaText>
                        </NotificationMessage>
                      </RuleElementDef>
                    </SubRules>
                    <HasContextVersionSettings>false</HasContextVersionSettings>
                  </RuleElementDef>
                  <RuleElementDef
                    xsi:type="SimpleRuleDef"
                    Revision="1"
                    PublicRevision="1"
                    Guid="b6b011c7-98ab-40b2-92ad-b7cc4eacf5fb"
                    Name="Rule2">
                    <Condition>
                      <ReturnType>Boolean</ReturnType>
                      <FormulaText>IsRegexMatch(EmailString, "(\w+@[a-zA-Z_]+?\.[a-zA-Z]{2,6})")</FormulaText>
                    </Condition>
                    <SubRules>
                      <RuleElementDef
                        xsi:type="FireNotificationActionDef"
                        Revision="1"
                        PublicRevision="1"
