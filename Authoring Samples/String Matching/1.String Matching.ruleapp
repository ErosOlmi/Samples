                Name="RegExMatchExamples">
                <Condition>
                  <ReturnType>Boolean</ReturnType>
                  <FormulaText>true</FormulaText>
                </Condition>
                <SubRules>
                  <RuleElementDef
                    xsi:type="SimpleRuleDef"
                    Revision="1"
                    PublicRevision="1"
                    Guid="8d2e73e9-7cc9-41b5-90b9-b6b92ac41a10"
                    Name="Rule1">
                    <Condition>
                      <ReturnType>Boolean</ReturnType>
                      <FormulaText>IsRegexMatch(TargetString, "[a-z]{26}", true)</FormulaText>
                    </Condition>
                    <SubRules>
                      <RuleElementDef
                        xsi:type="FireNotificationActionDef"
                        Revision="1"
                        PublicRevision="1"
